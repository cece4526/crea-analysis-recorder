{
    "require": {
        "symfony/webapp-pack": "^1.3",
        "symfony/orm-pack": "^2.4",
        "symfony/form": "^7.3",
        "symfony/validator": "^7.3",
        "symfony/twig-bundle": "^7.3",
        "symfony/runtime": "^7.3",
        "symfony/yaml": "^7.3",
        "doctrine/doctrine-bundle": "^2.16",
        "twig/twig": "^3.21",
        "symfony/process": "^7.3",
        "symfony/webpack-encore-bundle": "^2.3"
    },
    "require-dev": {
        "symfony/maker-bundle": "^1.64",
        "phpunit/phpunit": "^12.0",
        "phpstan/phpstan": "^2.1"
    },
    "config": {
        "allow-plugins": {
            "symfony/runtime": true
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    }
}
