{
    "require": {
        "symfony/webapp-pack": "^1.3",
        "symfony/orm-pack": "^2.4",
        "symfony/form": "^7.3",
        "symfony/validator": "^7.3",
        "symfony/twig-bundle": "^7.3",
        "symfony/runtime": "^7.3"
    },
    "require-dev": {
        "symfony/maker-bundle": "^1.64",
        "phpunit/phpunit": "^10.0"
    },
    "config": {
        "allow-plugins": {
            "symfony/runtime": true
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    }
}
